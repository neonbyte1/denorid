{
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "workspace":[
    "./packages/core",
    "./packages/logger"
  ],
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "@std/fmt": "jsr:@std/fmt@^1.0.6"
  },
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "lint": {
    "rules": {
      "tags": ["recommended", "jsr"]
    },
    "exclude": ["node_modules/"]
  }
}
